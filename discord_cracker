import time
import requests
import random

from bs4 import BeautifulSoup

def generate_random_code(length=16):
    characters = "1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
    return ''.join(random.choices(characters, k=length))
qw = requests.get("https://discord.com/gifts/FoxfYIzv5q40rBfV")
ssss = BeautifulSoup(qw.content, "html.parser")
def check_gift_code(code):

    url = f"https://discord.com/gifts/{code}"
    response = requests.get(url, timeout=5)
    """if :
        print(f"nope {url}")
    else:
        print(f"success {url}")"""
    print(ssss.find("div","appMount_ea7e65"))
def main():
    num_codes = 10
    for _ in range(num_codes):
        code = generate_random_code()
        check_gift_code(code)
        time.sleep(1)

if __name__ == "__main__":
    main()
